{
  "identifier": "default",
  "description": "Разрешения для главного окна: core, http, shell.open",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://localhost:*" },
        { "url": "http://127.0.0.1:*" },
        { "url": "https://*" }
      ]
    },
    "shell:allow-open"
  ]
}
