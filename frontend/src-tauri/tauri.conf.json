{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "OS-LukintrLab",
  "version": "1.0.0",
  "identifier": "com.lukintrlab.os",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "OS-LukinterLab",
        "width": 1280,
        "height": 800,
        "resizable": true
      }
    ],
    "security": {
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.ico"],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "OS LukintrLab — офисный комплект и канбан для команд.",
    "shortDescription": "Офисный комплект OS LukintrLab",
    "category": "Productivity",
    "copyright": "",
    "licenseFile": null
  },
  "plugins": {}
}
